{
  "name": "@redegade/feem-xml",
  "version": "0.0.1",
  "description": "Validate, parse, and build XML files that follow the FEEM Standard.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "npm run test:jest && npm run lint",
    "test:jest": "jest",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "author": "Red Kurti <contact@kurti.red> (https://kurti.red)",
  "repository": {
    "type": "git",
    "url": "https://github.com/redegade/feem-xml.git"
  },
  "keywords": [
    "feem",
    "xml",
    "parser",
    "validator",
    "track",
    "trace",
    "edi"
  ],
  "license": "MIT",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "@types/node": "20.11.5",
    "eslint-plugin-jest": "^28.4.0",
    "gts": "^5.3.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "fast-xml-parser": "^4.3.6"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "coverageProvider": "v8",
    "moduleNameMapper": {
      "^(.*)\\.js$": [
        "$1",
        "$1.js"
      ]
    }
  }
}
